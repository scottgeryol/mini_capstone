@products.map do |product|
  {
    id: product.id,
    name: product.name,
    price: product.price,
    image_url: product.image_url,
    description: product.description,
    color: product.color,
    weight: product.weight,
    tracklist: product.tracklist,
    genre: product.genre,
    size: product.size,
    speed: product.speed,
    year: product.year,
    label: product.label,

  }
end
